---
const { url, nameSocial, logohref } = Astro.props;
---

<style is:global>
  .social-rectangle {
    display: flex;
    align-items: center;
    gap: 10px;
    padding: 0.5rem 1rem;
    text-decoration: none;
    border-color: whitesmoke;
    border: 1px solid;
  }
  .social-rectangle img {
    width: 35px;
    height: 35px;
  }

</style>
<div class="social-rectangle">
  <a href={url}>{nameSocial}</a>
  <img src={logohref} />
</div>
